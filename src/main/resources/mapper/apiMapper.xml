<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.api">
  <!-- 전기차 데이터 넣기 -->
  <insert id="insertInfo" parameterType="hashmap">
    insert into charginginfo
    values (null, #{addr}, #{bnm}, #{busiCall}, #{busiId}, #{busiNm}, #{chgerId}, #{chgerType}, #{delDetail}, #{delYn}, #{lastTedt}, #{lastTsdt}, #{lat}, #{limitDetail}, #{limitYn}, #{lng}, #{location}, #{method}, #{note}, #{nowTsdt}, #{output}, #{parkingFree}, #{powerType}, #{stat}, #{statId}, #{statNm}, #{statUpdDt}, #{useTime}, #{zcode})
  </insert>
</mapper>